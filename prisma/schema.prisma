// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model City {
  id    Int     @id @default(autoincrement())
  name String // @unique
  image String // @unique
  country String
  nameChinese String? // @unique
  score Float
  region String
}